{
  "name": "eslint-config-airbnb-typescript-vue",
  "version": "2.3.0",
  "description": "Airbnb Base Vue Typescript config",
  "license": "MIT",
  "author": "JosÃ© Manuel Casani Guerra <jmpl3.soporte@gmail.com>",
  "main": "index.js",
  "scripts": {
    "format": "yarn run format:package && yarn run format:prettier && yarn run format:eslint",
    "format:eslint": "eslint --fix .",
    "format:package": "prettier-package-json --write",
    "format:prettier": "prettier --write \"**/*.{js,json,md,yml}\"",
    "lint": "eslint ./",
    "postpublish": "pinst --enable",
    "prepublishOnly": "pinst --disable",
    "validate": "yarn run lint"
  },
  "dependencies": {
    "eslint": "^8.15.0",
    "eslint-config-airbnb-typescript-base": "^3.1.3",
    "eslint-plugin-vue": "^8.7.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.4",
    "commitlint-config-gitmoji": "^2.2.5",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.1",
    "lint-staged": "^12.4.1",
    "pinst": "^3.0.0",
    "prettier": "^2.6.2",
    "prettier-package-json": "^2.6.3",
    "typescript": "^4.6.4"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint"
  },
  "packageManager": "yarn@3.2.0"
}
